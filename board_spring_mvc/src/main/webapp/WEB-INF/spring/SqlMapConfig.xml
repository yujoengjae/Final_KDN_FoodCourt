<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- MyBatis Query를 위한 Xml에서 resultType, parametertype 으로 
		 사용할  DTO(VO) 클래스에 대한 별칭 설정
		   
		 Java API에서 자주 사용되는 클래스를 ibatis에서 미리 별칭으로 작성됨. 
		    => org.apache.ibatis.type.TypeAliasRegistry  참조
		 -->
		 
	 	<typeAlias type="com.kdn.model.domain.Board" alias="board"/>
	 	<typeAlias type="com.kdn.model.domain.NoticeBoard" alias="noticeBoard"/>
		<typeAlias type="com.kdn.model.domain.BoardFile" alias="boardfile"/>
		<typeAlias type="com.kdn.model.domain.NoticeBoardFile" alias="noticeBoardFile"/>
		<typeAlias type="com.kdn.model.domain.PageBean" alias="pagebean"/>
		<typeAlias type="com.kdn.model.domain.ReviewPageBean" alias="reviewpagebean"/>
		<typeAlias type="com.kdn.model.domain.NoticePageBean" alias="noticepagebean"/>
		<typeAlias type="com.kdn.model.domain.Member" alias="member"/>
		<typeAlias type="com.kdn.model.domain.Food" alias="food"/>
		<typeAlias type="com.kdn.model.domain.FoodMenu" alias="foodMenu"/>
		<typeAlias type="com.kdn.model.domain.Diet" alias="diet"/>
		<typeAlias type="com.kdn.model.domain.Suyo" alias="suyo"/>
		<typeAlias type="com.kdn.model.domain.AnonymityPageBean" alias="anonymitypagebean"/>
		<typeAlias type="com.kdn.model.domain.Anonymity" alias="anonymity"/>
		<typeAlias alias="review" type="com.kdn.model.domain.Review"/>
		<typeAlias alias="ranking" type="com.kdn.model.domain.Ranking"/>
		<typeAlias alias="counter" type="com.kdn.model.domain.Counter"/>
		<typeAlias alias="event" type="com.kdn.model.domain.Event"/>
		<typeAlias alias="rankingpagebean" type="com.kdn.model.domain.RankingPageBean"/>
			 
	</typeAliases>
	<mappers>
	<!-- sql 쿼리를 작성한 xml 파일 경로 설정 
	   => 요즘 트랜드가 SqlSessionFactoryBean에 속성정보로 설정한다. 
	 -->
	</mappers>
</configuration>


















